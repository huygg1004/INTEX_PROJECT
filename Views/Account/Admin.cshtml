@{
}
@model IEnumerable<ApplicationUser>


<header class="masthead" style="background-image: url('https://christineelder.com/wp-content/uploads/2016/10/king-tut-adobe-1080x632.jpg') ">
    <div class="overlay"></div>
    <div class="container">
        <div class="row">
            <div class="col-lg-8 col-md-10 mx-auto">
                <div class="site-heading">
                    <h1>BYU Fag el-Gamous Burial Information</h1>
                    <span class="subheading">View, Add and Edit Information</span>
                </div>
            </div>
        </div>
    </div>
</header>
<div class="container">
    <table class="table-bordered">
        <tr class="thead-light">
            <th>Email</th>
            <th>User is Editor</th>
            <th>User Access Failed</th>
        </tr>
        @foreach (var u in Model)
        {
            <tr>
                <td>@u.Email</td>
                <td>@u.Editor</td>
                <td>@u.AccessFailedCount</td>
                <td>
                    <form asp-action="Remove" method="Post">
                        <input name="userToDeleteEmail" value="@u.Email" />
                        <button type="submit">Delete</button>
                    </form>
                </td>
            </tr>
        }
    </table>

</div>